<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <data noupdate="1">

            <record id="received_payments" model="ir.cron">
                <field name="name">Received Payments (Invoice)</field>
                <field name="model_id" ref="model_account_move" />
                <field name="state">code</field>
                <field name="active">0</field>
                <field name="code">model.get_pending_invoicess()</field>
                <field name="interval_number">1</field>
                <field name="interval_type">minutes</field>
                <field name="numbercall">-1</field>
                <field eval="False" name="doall"/>
            </record>

            <record id="cron_load_payment_methode" model="ir.cron">
                <field name="name">Load Ebiz Payment Profiles</field>
                <field name="model_id" ref="model_res_partner" />
                <field name="state">code</field>
                <field name="active">0</field>
                <field name="code">model.cron_load_payment_methods()</field>
                <field name="interval_number">5</field>
                <field name="interval_type">minutes</field>
                <field name="numbercall">-1</field>
                <field eval="False" name="doall"/>
            </record>
        </data>


</odoo>