        <record id="wizard_multi_payment_message_view" model="ir.ui.view">
            <field name="name">wizard.multi.payment.message</field>
            <field name="model">wizard.multi.payment.message</field>
            <field name="arch" type="xml">
                <form string="Message">
                    <field name="name" invisible="1"/>
                    <div attrs="{'invisible':[('name','!=','send')]}">
                            <field name="success_count" nolabel="1" readonly="1"/> out of <field name="total" nolabel="1" readonly="1"/> request(s) were successfully sent!
                    </div>
                    <div attrs="{'invisible':[('name','!=','resend')]}">
                            <field name="success_count" nolabel="1" readonly="1"/> out of <field name="total" nolabel="1" readonly="1"/> request(s) were successfully resent!
                    </div>

                    <div attrs="{'invisible':[('name','!=','delete')]}">
                            <field name="success_count" nolabel="1" readonly="1"/> out of <field name="total" nolabel="1" readonly="1"/> request(s) for Payment Method(s) were successfully removed!
                    </div>
                    <br/>
                        <field name="lines_ids" readonly="1"  color="green:status=='Success'">
                            <tree>
                                <field name="customer_name"/>
                                <field name="customer_id"/>
                                <field name="status"  color="green:status=='Success'"/>
                            </tree>
                        </field>
                    <footer>
                        <button string="Ok" class="oe_highlight" special="cancel" style="width:10%"/>
                    </footer>
                </form>
            </field>
        </record>


        <record id="wizard_email_pay_message_view" model="ir.ui.view">
            <field name="name">wizard.email.pay.message</field>
            <field name="model">wizard.email.pay.message</field>
            <field name="arch" type="xml">
                <form string="Message">
                    <field name="name" invisible="1"/>
                    <div attrs="{'invisible':[('name','!=','email_pay')]}">
                            <field name="success_count" nolabel="1" readonly="1"/> out of <field name="total" nolabel="1" readonly="1"/> request(s) were successfully sent!
                    </div>
                    <div attrs="{'invisible':[('name','!=','resend_email_pay')]}">
                            <field name="success_count" nolabel="1" readonly="1"/> out of <field name="total" nolabel="1" readonly="1"/> request(s) were successfully resent!
                    </div>
                    <br/>
                        <field name="lines_ids" readonly="1"  color="green:status=='Success'">
                            <tree>
                                <field name="number"/>
                                <field name="customer_name"/>
                                <field name="customer_id"/>
                                <field name="status"  color="green:status=='Success'"/>
                            </tree>
                        </field>
                    <footer>
