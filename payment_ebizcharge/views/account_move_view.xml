<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="form_view_account_move_ebizcharge" model="ir.ui.view">
            <field name="name">account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <header position='inside'>
                    <field name="ebiz_internal_id" invisible="1"/>
                    <field name="receipt_status" invisible="1"/>
                    <field name="ebiz_auto_sync" invisible="1"/>
                    <field name="ebiz_auto_sync_credit_note" invisible="1"/>
                    <field name="ebiz_transaction_id" invisible="1"/>
                    <field name="is_refund_processed" invisible="1"/>
                    <field name="payment_internal_id" invisible="1"/>
                    <field name="ebiz_invoice_status" invisible="1"/>
                    <field name="auto_received_schedular" invisible="1"/>
                    <field name="done_transaction_ids" invisible="1"/>
                    <field name="email_recieved_payments" invisible="1"/>
                    <button name="sync_to_ebiz_invoice" type="object"
                        string="EBizCharge Upload" class="oe_highlight"
                        attrs="{'invisible': ['|', '|', '|', ('ebiz_internal_id','!=',False), ('ebiz_auto_sync','=', True),('move_type','!=','out_invoice'),('state','=','draft')]}"/>

                    <button name="sync_to_ebiz_credit_note" type="object"
                        string="EBizCharge Upload" class="oe_highlight"
                        attrs="{'invisible': ['|', '|', '|', ('ebiz_internal_id','!=',False), ('ebiz_auto_sync_credit_note','=', True),('move_type','!=','out_refund'),('state','=','draft')]}"/>

                    <button name="email_invoice_ebiz" type="object"
                        string="Email Pay Request" class="oe_highlight"
                            attrs="{'invisible': ['|','|','|',
                            ('state','=','cancel'),
                            ('payment_state','=','paid'),
                            ('ebiz_invoice_status','=','pending'),
                            ('move_type','!=','out_invoice')

                            ]}"
                        />


                    <button name="resend_email_invoice_ebiz" type="object"
                        string="Resend Email Pay Request" class="oe_highlight"
                        attrs="{'invisible': ['|','|','|',
                        ('ebiz_invoice_status','!=','pending'),
                        ('payment_state','=','paid'),
                        ('email_recieved_payments','=',True),
                        ('move_type','!=','out_invoice')

                            ]}"

                        />

                    <button name="get_pending_invoicess" type="object"
                        string="Apply Received Payments" class="oe_highlight"
                            attrs="{'invisible': ['|','|','|','|',
                            ('payment_internal_id','=',False),
                            ('ebiz_invoice_status','!=','pending'),
                            ('payment_state','=','paid'),
                            ('email_recieved_payments','=',False),
                            ('move_type','!=','out_invoice')

                            ]}"
                        />

                    <button name="show_pending_ebiz_email" type="object"
                        string="View Pending Requests" class="oe_highlight"
                            attrs="{'invisible': ['|','|','|',
                            ('ebiz_invoice_status','!=','pending'),
                            ('email_recieved_payments','=',True),
                            ('payment_state','=','paid'),
                            ('move_type','!=','out_invoice')
                            ]}"
                        />


                    <button name="delete_ebiz_incvoice" type="object"
                        string="Cancel Email Pay Request" class="oe_highlight"
                            attrs="{'invisible': ['|','|','|',('ebiz_invoice_status','!=','pending'),
                            ('payment_state','=','paid'),
                            ('email_recieved_payments','=',True),
                            ('move_type','!=','out_invoice')

                            ]}"
                        />


                    <button name="email_receipt_ebiz" type="object"
                        string="Email Receipt" class="oe_highlight"
                            attrs="{'invisible': ['|',('receipt_status','=', False),
                            ('move_type','!=','out_invoice')

                            ]}"
                        />

                    <button name="show_ebiz_invoice" type="object"
                        string="View EBizCharge Invoice" class="oe_highlight"
                            attrs="{'invisible': [('ebiz_internal_id','=',False)]}"
                        />
                </header>
                <field name="partner_id" position="after">
                    <field name="sync_status" readonly='1'/>
                </field>
            </field>
        </record>

        <record id="model_action_wizard_invoice_multi_sync" model="ir.actions.server">
            <field name="name">EBizCharge Upload</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="binding_model_id" ref="account.model_account_move"/>
            <field name="binding_view_types">list</field>
            <field name="state">code</field>
            <field name="code">action = records.ebiz_sync_multiple_invoices()</field>
        </record>

        <record id="view_sale_order_ebizcharge_status" model="ir.ui.view">
            <field name="name">account.move.ebiz.status</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_invoice_tree"/>
            <field name="arch" type="xml">
                <tree>
                    <field name="ebiz_invoice_status"></field>
                    <field name="sync_status" optional="show"></field>
                </tree>
            </field>
        </record>

        <record id="view_account_payment_form_inherit_payment_main" model="ir.ui.view">
            <field name="name">view.account.payment.form.inherit.payment</field>
            <field name="model">account.payment</field>
            <field name="inherit_id" ref="account.view_account_payment_form"/>
            <field name="arch" type="xml">
                <field name="payment_transaction_id" position="attributes">
                    <attribute name="groups">base.group_user</attribute>
                </field>
                <xpath expr='//group[2]' position="inside">
                        <field name="transaction_ref" attrs="{'invisible': [('payment_method_code', '!=', 'ebizcharge')]}"/>
                    </xpath>
            </field>

        </record>

        <record id="view_account_move_reversal_update" model="ir.ui.view">
            <field name="name">account.move.reversal.form</field>
            <field name="model">account.move.reversal</field>
            <field name="inherit_id" ref="account.view_account_move_reversal"/>
            <field name="arch" type="xml">
                <xpath expr="/form/group[1]/group[1]" position="attributes">
                    <attribute name="attrs">{'invisible': [('residual', '=', 0)]}</attribute>
                </xpath>
                <xpath expr="//button[@name='reverse_moves']" position="attributes">
                    <attribute name="string">Process</attribute>
                </xpath>
            </field>
        </record>
        <record id="view_invoice_upload_tree" model="ir.ui.view">
            <field name="name">res.partner.form</field>
            <field name="model">account.move</field>
            <field name="arch" type="xml">
                <tree create="0">
                    <field name = "name"/>
                    <field name = "currency_id" invisible='1'/>
                    <field name = "state" invisible='1'/>
                    <field name = "invoice_partner_display_name" string="Customer"/>
                    <field name = "customer_id" string="Customer ID"/>
                    <field name = "amount_total_signed" string="Invoice Total" options="{'currency_field': 'currency_id'}" widget="monetary"/>
                    <field name = "amount_residual_signed" string="Balance Remaining" options="{'currency_field': 'currency_id'}" widget="monetary"/>
                    <field name = "invoice_date" string="Invoice Date"/>
                    <field name = "invoice_date_due"/>
                    <field name = "last_sync_date"/>
                    <field name = "sync_response"/>
                </tree>
            </field>
        </record>

        <record id="action_invoice_sync_tree" model="ir.actions.act_window">
            <field name="name">Upload Invoices</field>
            <field name="res_model">account.move</field>
            <field name='view_mode'>tree</field>
            <field name='domain'>[["type", "=", "out_invoice"],['state','=','posted']]</field>
            <field name="context">{'ebiz_upload_invoice_action':1}</field>
            <field name='view_id' ref="view_invoice_upload_tree"/>
        </record>

