<odoo>
  <data>
            <!--Display form view of wizard-->    
        <record id="view_material_report_wizard_form" model="ir.ui.view">
            <field name="name">material.report.wizard.form</field>
            <field name="model">material.report.wizard</field>
            <field name="arch" type="xml">
                <form string="Material Request Report">
                    <group>  
                       <group>
                              <field name="date_from" />
                              <field name="partner_id" />
                      </group>
                        
                      <group>
                              <field name="date_to" />
                              <field name="status" />
                      </group>                          
                    </group>
                    <footer>
                        <button string='Print' name="check_report" type="object" default_focus="1" class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

           <!--Open the date wise wizard on click of menu-->
        <record id="open_material_request_wizard_action" model="ir.actions.act_window">
            <field name="name">Material Request Report</field>
            <field name="res_model">material.report.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="view_material_report_wizard_form"/>
        </record>
      
      
      <menuitem id="menu_material_request_reptprt" name="Reporting" parent="claimed_form.claimed_form_menu_item_id"  
             sequence="10"/>
      
       <menuitem id="menu_material_request_rept" name="Claim Report" parent="menu_material_request_reptprt"  action='open_material_request_wizard_action'
             sequence="10"/>
      
      
      
  </data>
</odoo>